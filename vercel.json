{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "header",
          "key": "x-forwarded-proto",
          "value": "http"
        }
      ],
      "permanent": true,
      "destination": "https://$1"
    }
  ],
  "rewrites": [
       {
      "source": "/app/service-worker.js",
      "destination": "/app/service-worker.js"
    },
    {
      "source": "/app",
      "destination": "/public/index.html"
    },
    {
      "source": "/app/:path*",
      "destination": "/public/index.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/.well-known/assetlinks.json",
      "destination": "/public/.well-known/assetlinks.json"
    },
    {
      "source": "/manifest.webmanifest",
      "destination": "/manifest.webmanifest"
    },
    {
    "source": "/support.html",
    "destination": "/public/support.html"
  },
  {
    "source": "/privacy.html",
    "destination": "/public/privacy.html"
  },
    {
      "source": "/icons/:path*",
      "destination": "/icons/:path*"
    }
    
  ],
  "headers": [
   {
      "source": "/app/service-worker.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript; charset=utf-8"
        }
      ]
    },
    {
      "source": "/.well-known/assetlinks.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    },
    {
      "source": "/manifest.webmanifest",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/manifest+json"
        }
      ]
    },
    
    {
      "source": "/icons/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}